<table>
    <thead>
    <tr>
        <th>User name</th>
        <th>Source</th>
        <th>Diabetes key</th>
        <th>Antibody</th>
        <th>Note</th>
        <th>Created at</th>
        <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @personal_records.each do |pr| %>
        <tr>
            <td><%= pr.user.name %></td>
            <td><%= pr.source %></td>
            <td><%= pr.diabetes_key %></td>
            <td><%= pr.antibody_key %></td>
            <td><%= pr.note %></td>
            <td><%= pr.created_at %></td>
        </tr>
    <% end %>
    </tbody>
</table>

<br>

