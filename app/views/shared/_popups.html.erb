<div id="infoPopup" class="dataCaptured hidden">
  <div class="inner">
    <center>
      <i class="fa fa-check-circle-o fa-5x info-icon"></i>
    </center>
    <div style="text-align: center;font-size: 24px;padding-top: 20px;"><%=(t :done)%></div>
    <div style="text-align: center;padding-left: 20px;padding-right: 20px;padding-top: 20px;color: #bababa;">
      <span class="msg"></span>
    </div>
  </div>
  <div class="bottom">
    <center>
      <button id="infoOkButton" class="infoButton">OK</button>
    </center>
  </div>
</div>

<div id="errorPopup" class="dataCaptured hidden">
  <div class="inner">
    <center>
      <i class="fa fa-times-circle-o fa-5x error-icon"></i>
    </center>
    <div style="text-align: center;font-size: 24px;padding-top: 20px;"><%=(t :failed)%></div>
    <div style="text-align: center;padding-left: 20px;padding-right: 20px;padding-top: 20px;color: #bababa;">
      <span class="msg"></span>
    </div>
  </div>
  <div class="bottom">
    <center>
      <button id="errorOkButton" class="infoButton">OK</button>
    </center>
  </div>
</div>
